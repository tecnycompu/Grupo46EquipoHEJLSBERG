@page
@model ClinicaLitleCats.App.Frontend.Pages.ListModel
@{
}
<h1>Lista de clientes</h1>

<table class="table">
    
    @foreach (var propietarioEncargado in Model.propietarioEncargados)
    {
       <tr>
           <td>@propietarioEncargado.Nombre</td>
           <td>@propietarioEncargado.Apellido</td>
           <td>
                <a class="btn btn-primary table-btn" asp-page="./Details" asp-route-propietarioEncargadoId="@propietarioEncargado.Id">
                   <i class="fas fa-info-circle fa-lg"></i>
                 </a>
           </td>

            <td>
                <a class="btn btn-primary table-btn" asp-page="./Edit" asp-route-propietarioEncargadoId="@propietarioEncargado.Id">
                    <i class="fas fa-address-book fa-lg" aria-hidden="true"></i>
                </a>
           </td  
           </tr>


    }
    

</table>
<a asp-page=".\Edit" class="btn btn-primary">Adicionar un Propietario</a>

